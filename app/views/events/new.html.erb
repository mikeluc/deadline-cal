

<div class="box col-8 offset-2">
  <h1>Enter the new event details </h1>
  <%= simple_form_for @event, url: events_path do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :datetimedue %>
    <br />
    <%= f.submit 'create', class: 'btn btn-primary'%>
  <% end %>

</div>